<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>$(TfmGroupDotNetStandard)</TargetFrameworks>
	</PropertyGroup>

	<PropertyGroup>
		<Description>Library for opening and creating files in SLK format.</Description>
		<PackageTags>slk;warcraft3</PackageTags>
	</PropertyGroup>

	<ItemGroup Condition="'$(IsPublishBuild)'!='true'">
		<ProjectReference Include="..\War3Net.Common\War3Net.Common.csproj" />
	</ItemGroup>

	<ItemGroup Condition="'$(IsPublishBuild)'=='true'">
		<PackageReference Include="War3Net.Common" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)'=='netstandard2.0'">
		<Compile Include="..\..\submodules\PolySharp\src\PolySharp.SourceGenerators\EmbeddedResources\System.Diagnostics.CodeAnalysis.DoesNotReturnAttribute.cs" Link="PolySharp\System.Diagnostics.CodeAnalysis.DoesNotReturnAttribute.cs" />
		<Compile Include="..\..\submodules\PolySharp\src\PolySharp.SourceGenerators\EmbeddedResources\System.Diagnostics.CodeAnalysis.NotNullWhenAttribute.cs" Link="PolySharp\System.Diagnostics.CodeAnalysis.NotNullWhenAttribute.cs" />
		<Compile Include="..\..\submodules\PolySharp\src\PolySharp.SourceGenerators\EmbeddedResources\System.Index.cs" Link="PolySharp\System.Index.cs" />
		<Compile Include="..\..\submodules\PolySharp\src\PolySharp.SourceGenerators\EmbeddedResources\System.Range.cs" Link="PolySharp\System.Range.cs" />
	</ItemGroup>

</Project>
